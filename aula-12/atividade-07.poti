var ve := vetor[100]
var numero := -1
var i := 0

enquanto numero <> 0 faça
	numero := leia_inteiro
    se numero <> 0 então
    	ve[i] := numero
        i := i + 1
        fim
 fim
 
 para j de 0 até i-1 faça
 	imprima "{ve[j]} "
fim

para k de 0 até i-1 faça
	se ve[k] < 0 então
    	ve[k] := -ve[k]
     fim
fim

escreva ""
para m de i-1 até 0 passo -1 faça
	imprima "{ve[m]} "
fim