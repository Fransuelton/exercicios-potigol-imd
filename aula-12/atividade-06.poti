var v := vetor[20]
var soma := 0

para i de 0 até 19 faça
	v[i] := leia_numero
    soma := soma + v[i]
fim
var media := soma/20
escreva media

para i de 0 até 19 faça
	se v[i] > media então
    	imprima "{v[i]} "
     fim
fim
escreva ""

para i de 0 até 19 faça
	se v[i] < media então
    	imprima "{v[i]} "
     fim
fim