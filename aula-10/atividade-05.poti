var num := leia_inteiro
var contador := 0

para i de 2 até num faça
	contador := 0
    para j de 1 até i faça
    	se i mod j == 0 então
        	contador := contador + 1
         fim
      fim
      se contador == 2 então
      	escreva "{i} primo"
     senão
     	escreva "{i} composto"
        para k de 2 até i faça
        	se i mod k == 0 |e| i <> k então
            	escreva k
             fim
          fim
       fim
    fim